{
    "name": "aws-lambda-golang-serverless-framework",
    "version": "1.0.0",
    "description": "Sample serverless CRUD service exposing a REST HTTP interface",
    "author": "",
    "license": "MIT",
    "scripts": {
        "start": "sam local start-api --env-vars env.json",
        "watch": "nodemon --watch ./ -e go,js --exec make build",
        "local-invoke": "sam local invoke ListToDoFunctions -e event.json",
        "debug": "npm-run-all --parallel start watch",
        "offline": "serverless offline",
        "start-with-docker": "node ./node_modules/.bin/serverless offline start --useDocker"
    },
    "devDependencies": {
        "nodemon": "^2.0.15",
        "npm-run-all": "^4.1.5",
        "serverless": "^2.72.3",
        "serverless-localstack": "^0.4.35",
        "serverless-offline": "^8.5.0"
    }
}
